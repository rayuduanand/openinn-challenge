{
  "name": "tic-tac-toe-typescript",
  "version": "1.0.0",
  "description": "Tic-tac-toe game implemented in TypeScript",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc && tsc --project tsconfig.client.json",
    "start": "node dist/app.js",
    "dev": "concurrently \"ts-node-dev --respawn src/app.ts\" \"tsc --project tsconfig.client.json --watch\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "tic-tac-toe",
    "typescript",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "applicationinsights": "^2.9.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
}
